void hardware_init(){
  
  // UART1_Init
  setPinFun(2,FORTH_FUNCTION);
  setPinFun(3,FORTH_FUNCTION);
  Serial1.begin(9600);
  
  // UART2_Init 
  setPinFun(5,FORTH_FUNCTION);
  setPinFun(6,FORTH_FUNCTION);
  Serial2.begin(9600);
  
  // WS2812_Init
  ASR_WS2812.pixel_init(30+256);
  
  // Param_Init
  volume = 4;
  brightNess = 255;
  vol_set(volume);
  
  // Timer_Init
  timerSleepHandler = xTimerCreate("timerSleep", 1000 * 60 * 30, 0, 0, timerSleepProc);
  
  // xTaskCreate(app,"app",128,NULL,4,NULL);
  
  // Rainbow_Create
  xTaskCreate(rainbowProc,"rainbow",128,NULL,4,&rainbowProcHandler);
  
  vTaskSuspend(rainbowProcHandler);
  
  vTaskDelete(NULL);
}

